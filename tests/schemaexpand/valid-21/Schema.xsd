<?xml version="1.0"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <xs:simpleType name="md5hashtype">
    <xs:restriction base="xs:string">
      <xs:pattern value="[0-9A-F]{32}"/>
      <xs:length value="32"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="topictype">
    <xs:restriction base="xs:string">
      <xs:pattern value="[0-9]{1,2}(-[0-9]{1,5}){0,1}"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="phonetype">
    <xs:restriction base="xs:string">
      <xs:pattern value="([0-9][\s0-9-]{4,}[0-9]|\w+)"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="timeperiodtype">
    <xs:restriction base="xs:string">
      <xs:enumeration value="В любое время"/>
      <xs:enumeration value="В рабочее время"/>
      <xs:enumeration value="Круглосуточно"/>
      <xs:enumeration value="Без выходных"/>
      <xs:enumeration value="В будние дни"/>
      <xs:enumeration value="В выходные дни"/>
      <xs:enumeration value="Утром"/>
      <xs:enumeration value="Днем"/>
      <xs:enumeration value="Вечером"/>
      <xs:enumeration value="Время московское"/>
      <xs:enumeration value="Понедельник-Пятница"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="timerangetype">
    <xs:sequence>
      <xs:element name="from" type="xs:time" minOccurs="0" maxOccurs="1"/>
      <xs:element name="to" type="xs:time" minOccurs="0" maxOccurs="1"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="timetype">
    <xs:sequence>
      <xs:element name="period" type="timeperiodtype" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="range" type="timerangetype" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:simpleType name="timeunittype">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Месяц"/>
      <xs:enumeration value="Год"/>
    </xs:restriction>
  </xs:simpleType>

