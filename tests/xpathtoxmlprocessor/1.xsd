  <xs:element name="drives">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="drive" maxOccurs="unbounded">
          <xs:annotation>
            <xs:appinfo>
              <vxe:settings>
                <vxe:instanceWrapperClass>drive</vxe:instanceWrapperClass>
                <vxe:startFoldedInstance>true</vxe:startFoldedInstance>
                <vxe:compoundTitle>model brand modelname</vxe:compoundTitle>
                <vxe:titleClass>title</vxe:titleClass>
                <vxe:compoundTitleHideTitles>true</vxe:compoundTitleHideTitles>
                <vxe:showFoldInstanceButton>true</vxe:showFoldInstanceButton>
              </vxe:settings>
            </xs:appinfo>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="model" type="notemptystring">
                <xs:annotation>
                  <xs:documentation>
                    <dc:title xml:lang="en">Drive model</dc:title>
                  </xs:documentation>
                  <xs:appinfo>
                    <vxe:settings>
                      <vxe:widget>
                        <vxe:singleline/>
                      </vxe:widget>
                    </vxe:settings>
                  </xs:appinfo>
                </xs:annotation>
              </xs:element>
              <xs:element name="brand" type="brandtype">
                <xs:annotation>
                  <xs:documentation>
                    <dc:title xml:lang="en">Brand name</dc:title>
                  </xs:documentation>
                  <xs:appinfo>
                    <vxe:settings>
                      <vxe:widget>
                        <vxe:select>
                          <vxe:sortOptionsByName>true</vxe:sortOptionsByName>
                          <vxe:allowEmptyElement>true</vxe:allowEmptyElement>
                          <vxe:emptyElementText>Please select</vxe:emptyElementText>
                        </vxe:select>
                      </vxe:widget>
                    </vxe:settings>
                  </xs:appinfo>
                </xs:annotation>
              </xs:element>
              <xs:element name="modelname" type="notemptystring">
                <xs:annotation>
                  <xs:documentation>
                    <dc:title xml:lang="en">Model name</dc:title>
                  </xs:documentation>
                  <xs:appinfo>
                    <vxe:settings>
                      <vxe:widget>
                        <vxe:singleline/>
                      </vxe:widget>
                    </vxe:settings>
                  </xs:appinfo>
                </xs:annotation>
              </xs:element>
              <xs:element name="formfactor" type="formfactortype" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>
                    <dc:title xml:lang="en">Form factor</dc:title>
                  </xs:documentation>
                  <xs:appinfo>
                    <vxe:settings>
                      <vxe:showAddDeleteButtons>false</vxe:showAddDeleteButtons>
                      <vxe:widget>
                        <vxe:select>
                          <vxe:sortOptionsByName>true</vxe:sortOptionsByName>
                          <vxe:allowEmptyElement>true</vxe:allowEmptyElement>
                          <vxe:emptyElementText>Please select</vxe:emptyElementText>
                        </vxe:select>
                      </vxe:widget>
                    </vxe:settings>
                  </xs:appinfo>
                </xs:annotation>
              </xs:element>
              <xs:element name="numbers">
                <xs:annotation>
                  <xs:documentation>
                    <dc:title xml:lang="en">Numbers</dc:title>
                  </xs:documentation>
                  <xs:appinfo>
                    <vxe:settings>
                      <vxe:startFolded>true</vxe:startFolded>
                      <vxe:showFoldButton>true</vxe:showFoldButton>
                    </vxe:settings>
                  </xs:appinfo>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="number" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="description" type="numbersdescriptiontype">
                            <xs:annotation>
                              <xs:appinfo>
                                <vxe:settings>
                                  <vxe:widget>
                                    <vxe:select>
                                      <vxe:sortOptionsByName>true</vxe:sortOptionsByName>
                                      <vxe:allowEmptyElement>true</vxe:allowEmptyElement>
                                      <vxe:emptyElementText>Please select</vxe:emptyElementText>
                                    </vxe:select>
                                  </vxe:widget>
                                </vxe:settings>
                              </xs:appinfo>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="regexp" type="notemptystring">
                            <xs:annotation>
                              <xs:appinfo>
                                <vxe:settings>
                                  <vxe:widget>
                                    <vxe:singleline/>
                                  </vxe:widget>
                                </vxe:settings>
                              </xs:appinfo>
                            </xs:annotation>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>
